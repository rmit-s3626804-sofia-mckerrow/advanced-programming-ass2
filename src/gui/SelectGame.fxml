<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="463.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.SelectGameController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="44.0" layoutY="14.0" prefHeight="46.0" prefWidth="312.0" text="Select type of game to play">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="cycling" layoutX="85.0" layoutY="72.0" mnemonicParsing="false" onAction="#selectCycling" prefHeight="46.0" prefWidth="230.0" text="Cycling race">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="swimming" layoutX="85.0" layoutY="213.0" mnemonicParsing="false" onAction="#selectSwimming" prefHeight="46.0" prefWidth="230.0" text="Swimming race">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="running" layoutX="85.0" layoutY="144.0" mnemonicParsing="false" onAction="#selectRunning" prefHeight="46.0" prefWidth="230.0" text="Running race">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="addGameStatus" alignment="CENTER" contentDisplay="CENTER" layoutX="44.0" layoutY="269.0" prefHeight="46.0" prefWidth="312.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="raceID" alignment="CENTER" contentDisplay="CENTER" layoutX="44.0" layoutY="315.0" prefHeight="46.0" prefWidth="312.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="next" layoutX="85.0" layoutY="373.0" mnemonicParsing="false" onAction="#nextButtonClick" prefHeight="46.0" prefWidth="230.0" text="Next">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
